//stack using standard libeary

#include <iostream>
#include <stack> // Include the stack header

using namespace std;

int main(){
    int n, num;
    cin >> n; // Read the number of elements
    stack<int> st;
    for(int i = 0; i < n; i++){
        cin >> num;
        st.push(num);
    }
   
    while(!st.empty()){
        cout << st.top() << " "; // Print the top element
        st.pop(); // Remove the top element
    }
    return 0;
}

